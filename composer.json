{
    "name": "blogify/blogify",
    "description": "The Laravel blog package.",
    "keywords": [
        "laravel",
        "blog",
        "Blogify",
        "Blog package"
    ],
    "homepage": "https://github.com/blogify/blogify",
    "license": "MIT",
    "authors": [
        {
            "name": "Joren Van Hocht",
            "email": "joren@codeswitch.be",
            "homepage": "https://codeswitch.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "3.1.0|3.2.0|3.3.0"
    },
    "autoload": {
        "psr-4": {
            "Blogify\\Blogify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blogify\\Blogify\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blogify\\Blogify\\BlogifyServiceProvider"
            ]
        }
    }
}
